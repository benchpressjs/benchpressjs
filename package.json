{
  "name": "benchpressjs",
  "version": "2.0.9",
  "author": "psychobunny <psycho.bunny@hotmail.com>",
  "description": "An ultralight and super fast templating framework",
  "scripts": {
    "pretest": "grunt babel && eslint --cache .",
    "test": "nyc --reporter=html --reporter=text mocha -R spec tests",
    "coverage": "nyc report --reporter=text-lcov > ./coverage/lcov.info",
    "docs": "documentation build lib/benchpress.js lib/precompile.js lib/compile-render.js lib/express.js -f md -o docs/api.md --shallow",
    "prepare": "grunt build uglify",
    "prepublishOnly": "grunt",
    "postinstall": "cd rust/benchpress-rs && npm install || echo '[benchpress] Native module install failed. Will use the fallback JS compiler.'"
  },
  "repository": "git://github.com/benchpressjs/benchpressjs",
  "main": "build/lib/benchpress",
  "browser": "build/benchpress",
  "keywords": [
    "templates",
    "templating",
    "framework",
    "javascript"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-arrow-functions": "^7.10.4",
    "@babel/plugin-transform-block-scoped-functions": "^7.10.4",
    "@babel/plugin-transform-block-scoping": "^7.11.1",
    "@babel/plugin-transform-function-name": "^7.10.4",
    "@babel/plugin-transform-shorthand-properties": "^7.10.4",
    "async": "^3.2.0",
    "babel-eslint": "^10.1.0",
    "benchmark": "^2.1.4",
    "coveralls": "^3.1.0",
    "documentation": "^13.0.2",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "express": "^4.17.1",
    "grunt": "^1.3.0",
    "grunt-babel": "^8.0.0",
    "grunt-contrib-uglify": "^5.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-mocha-test": "^0.13.3",
    "mkdirp": "^1.0.4",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@babel/generator": "^7.11.6",
    "@babel/types": "^7.11.5",
    "lodash": "^4.17.20",
    "node-cache": "^5.1.2",
    "uglify-js": "^3.10.4"
  },
  "nyc": {
    "exclude": [
      "tests"
    ]
  }
}
